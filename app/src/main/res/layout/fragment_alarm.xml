<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bodered_blue_bg"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:fab="http://schemas.android.com/apk/res-auto"
    tools:context="com.competition.kakin.mtreatment.UI.Notifi.AlarmFragment"
    >
    <ListView
        android:layout_width="345dp"
        android:layout_height="match_parent"
        android:layout_marginTop="10dp"
        android:layout_centerHorizontal="true"
        android:id="@+id/medAlarmListView"
        android:divider="@null"
        android:dividerHeight="7dp"
        >
    </ListView>
    <com.getbase.floatingactionbutton.FloatingActionsMenu
        android:id="@+id/fabAlarm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        fab:fab_addButtonColorNormal="@color/fab_green"
        fab:fab_addButtonColorPressed="@color/white_pressed"
        fab:fab_addButtonPlusIconColor="@color/text_white"
        fab:fab_labelStyle="@style/menu_labels_style"
        android:layout_marginBottom="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginEnd="16dp">
        <com.getbase.floatingactionbutton.FloatingActionButton
            android:id="@+id/btn_alarm_history"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            fab:fab_colorNormal="@color/fab_yello"
            fab:fab_icon="@drawable/ic_fab_star"
            fab:fabSize="mini"
            fab:fab_title="历史提醒"
            fab:fab_colorPressed="@color/white_pressed">
        </com.getbase.floatingactionbutton.FloatingActionButton>
        <com.getbase.floatingactionbutton.FloatingActionButton
            android:id="@+id/btn_add_alarm"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            fab:fab_colorNormal="@color/fab_blue"
            fab:fab_icon="@drawable/ic_plus"
            fab:fabSize="mini"
            fab:fab_title="添加提醒"
            fab:fab_colorPressed="@color/white_pressed">
        </com.getbase.floatingactionbutton.FloatingActionButton>
    </com.getbase.floatingactionbutton.FloatingActionsMenu>

</RelativeLayout>
